    "dev": "node server.js --inspect",
    "swag": "restful-react import --url ethekwinibackend.boxfusion.co.za/swagger/v1/swagger.json --output useApis/index.tsx",
    "build": "next build",
    "export": "npm run build && next export",
    "export-hack": "npm install next@^7.0.3 && npm run build && npm run export && npm install next@latest",
    "start": "NODE_ENV=production node server.js",
    "test": "npm run test:units",
    "test:units": "NODE_ENV=testing jest --config tests/units/jest.config.js",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "precommit": "lint-staged",
    "changelog:major": "changelog -M && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version major && git push origin",
    "changelog:minor": "changelog -m && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version minor && git push origin",
    "changelog:patch": "changelog -p && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version patch && git push origin",
    "release": "git push origin --tags",
    "release-and-publish": "npm run release && npm publish",
    "generate-fetcher": "restful-react import --config restful-react.config.js",
    "postgenerate-fetcher": "prettier ./api/*.tsx --write"
  },
  "lint-staged": {
    "*.{js,jsx,json,css,ts,tsx}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
